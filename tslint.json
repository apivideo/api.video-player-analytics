{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "member-access": true,
        "member-ordering": [true, {"order": "fields-first"}],
        "no-console": false,
        "no-bitwise": false
    },
    "rulesDirectory": []   
}